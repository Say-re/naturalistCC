{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "plugin:react/recommended",
        "plugin:flowtype/recommended",
        "plugin:jsx-a11y/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react",
        "import",
        "flowtype",
        "jsx-a11y"
      ],
    "rules": {
      "array-bracket-spacing": [
        "error",
        "always",
        {
          "arraysInArrays": false,
          "objectsInArrays": false,
          "singleValue": false
        }
      ],
      "class-methods-use-this": [
        0
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "never"
        }
      ],
      "default-param-last": 0,
      "dot-notation": [
        1
      ],
      "func-style": [
        "error",
        "declaration",
        {
          "allowArrowFunctions": true
        }
      ],
      "react/prop-types": [
        0
      ],
      "jsx-a11y/label-has-for": "off",
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to",
            "hrefLeft",
            "hrefRight"
          ],
          "aspects": [
            "noHref",
            "invalidHref",
            "preferButton"
          ]
        }
      ],
      "jsx-a11y/no-onchange": [
        0
      ],
      "jsx-quotes": [
        "error",
        "prefer-single"
      ],
      "max-len": 0,
      "no-else-return": [
        "error",
        {
          "allowElseIf": false
        }
      ],
      "no-plusplus": [
        0
      ],
      "no-extra-boolean-cast": [
        0
      ],
      "no-dupe-else-if": [
        2
      ],
      "no-unreachable": [
        2
      ],
      "object-shorthand": [
        "error",
        "always"
      ],
      "prefer-arrow-callback": [
        "error",
        {
          "allowNamedFunctions": true
        }
      ],
      "react/jsx-closing-bracket-location": 0,
      "react/no-multi-comp": [
        2
      ],
      "space-before-function-paren": [
        "error",
        "always"
      ]
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true,
        "import/resolver": {
          "node": {
            "extensions": [
              ".js",
              ".jsx"
            ]
          }
        },
        "react": {
          "version": "16.4"
        }
      }
}
